{
  "name": "cvepdb/cms",
  "description": "The #CVEPDB CMS.",
  "keywords": [
    "cms",
    "cvepdb"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.9",
    "beelab/bowerphp": "0.3.*@beta",
    "composer/composer": "^1.0@beta",
    "composer/installers": "~1.0",
    "laravel/framework": "5.2.24",
    "doctrine/cache": "^1.6",
    "cvepdb/laravel": "dev-master",
    "cvepdb-cms/theme-adminlte": "master-dev",
    "cvepdb-cms/theme-lumen": "master-dev",
    "cvepdb-cms/theme-flatly": "master-dev",
    "cvepdb-cms/module-themes": "1.0.*",
    "spatie/laravel-medialibrary": "^3.16",
    "barryvdh/laravel-elfinder": "^0.3.6",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-rackspace": "~1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "league/factory-muffin": "^3.0",
    "league/factory-muffin-faker": "^1.0",
    "codeception/codeception": "*",
    "deployer/deployer": "^3.0",
    "deployphp/recipes": "~3.0",
    "phpmetrics/phpmetrics": "^1.10"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Core\\": "core/",
      "Modules\\": "modules/"
    },
    "files": [
      "core/helpers.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_put_contents('.env', 'installer');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ]
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  },
  "extra": {
    "installer-paths": {
      "resources/themes/{$name}": [
        "cvepdb-cms/theme-adminlte",
        "cvepdb-cms/theme-lumen",
        "cvepdb-cms/theme-flatly"
      ],
      "modules/{$name}": [
        "cvepdb-cms/module-themes"
      ]
    }
  }
}
